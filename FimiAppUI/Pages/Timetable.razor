                {
                    <MudTable Elevation="0" Items="@LessonCounts" FixedHeader="true" Hover="true" SortLabel="Sort By" Striped="true" T="LessonCountModel"Dense="true"Bordered="true">
                        <HeaderContent>
                            <MudTableSortLabel InitialDirection="SortDirection.Ascending" SortBy="new Func<LessonCountModel, object>(x=>x.Form)">Form</MudTableSortLabel>
                            <MudTh>Stream</MudTh>
                            <MudTh>Subject</MudTh>
                            <MudTh>Lesson count</MudTh>
                        </HeaderContent>
                        <RowTemplate>
                            <MudTd DataLabel="Form">@context.Form</MudTd>
                            <MudTd DataLabel="Stream">@context.Stream</MudTd>
                            <MudTd DataLabel="Subject">@context.Subject</MudTd>
                            <MudTd DataLabel="LessonCount">@context.LessonCount</MudTd>
                        </RowTemplate>
                        <PagerContent>
                            <MudTablePager />
                        </PagerContent>
                    </MudTable>
                }
            </MudItem>
        </MudGrid>
    </MudTabPanel>
